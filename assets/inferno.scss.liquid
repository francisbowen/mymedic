.collection_price_filter {
  margin-top: 60px;

  // Slider
  .slider-wrapper{
    width: 100%;
    display: inline-block;
    position: relative;
  }

  .ui-slider{
    background: #efefef;
    height: 10px;
    position: relative;
    border-radius: 100px;
  }

  .ui-slider-range{
    background: #71c0ff;
    height: 10px;
    position: absolute;
    transform: translateY(-1px);

    &:after{
      content: '';
      width: 100%;
      height: 1px;
      background: #fff;
      position: absolute;
      top: 0;
      left: 0;
      opacity: 0.3;
    }
  }

  .ui-slider-handle{
    background: #fff;
    border-color: #000;
    border: 1px solid;
    position: absolute;
    width: 20px;
    height: 20px;
    top: 50%;
    transform: translate(-50%,-50%);
    border-radius: 100px;
    cursor: -webkit-grab;

    &:focus{
      outline: none;
    }

    &:active{
      cursor: grabbing;
      cursor: -moz-grabbing;
      cursor: -webkit-grabbing;
    }
  }

  .ui-state-active{
    width: 22px;
  }



  // Range Indicator

  .range-wrapper{
    position: absolute;
    top: -50px;
    left: 50%;
    transform: translateX(-50%);
  }

  .range{
    background: #fff;
    white-space: nowrap;
    border: 1px solid #d2d2d2;
    border-radius: 2px;
    font-size: 13px;
    letter-spacing: 0.02em;
    color: #555;
    width: 150px;
    z-index: 10;
    position: relative;
  }

  .range-value{
    width: 74px;
    padding: 8px 0;
    text-align: center;
    display: inline-block;
    transition: background 0.1s;
  }

  .range-divider{
    width: 1px;
    display: inline-block;

    &:after{
      position: absolute;
      top: 0;
      left: 50%;
      width: 1px;
      height: 100%;
      background: #d2d2d2;
      content: '';
    }
  }

  .range-alert{
    width: 16px;
    height: 16px;
    position: absolute;
    top: 50%;
    right: 0;
    transform: translateY(-50%);
    background: #fff;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    border: 1px solid #d2d2d2;
    color: #0E0E0E;
    line-height: 15px;
    transition: right 0.2s;

    &.active{
      right: -15px;
    }
  }



  // Price Markers

  .marker{
    top: 100%;
    padding-top: 16px;
    font-size: 13px;
    color: #555;
    letter-spacing: 0.05em;

    &:after{
      content: '';
      width: 1px;
      height: 8px;
      background: #d2d2d2;
      position: absolute;
      top: 0;
      left: 50%;
      transform: translateX(-50%);
    }

    &.min_value {
      float: left;
      &:after{
        display: none;
      }
    }
  
    &.max_value {
      float: right;
  
      &:after{
        display: none;
      }
    }
  }
}