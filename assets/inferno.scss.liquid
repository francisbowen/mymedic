.fancybox-slide--iframe .fancybox-content {
	& { 
    max-width:100%;
    max-height:100%;
    position:absolute;
    width:75%;
    height:auto;
    padding-top:45%;
    left:50%;
    top:50%;
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    max-width:1400px;
    margin:0;
    box-sizing:border-box; 
  }
	iframe { 
    position:absolute;
    height:100%;
    width:100%;
    top:0;
    left:0;
    max-height:100%; 
  }
}

.fancybox-button.fancybox-button--share { display:none; }
.fancybox-button.fancybox-button--play { display:none; }
.fancybox-button.fancybox-button--fullscreen { display:none; }

@media all and (min-width:1400px) {
	.fancybox-slide--iframe .fancybox-content {
		& {
      height:700px;
      width:1150px;
      padding-top:0; 
    }
	}
}

@media all and (max-width:991px) {
	.fancybox-slide--iframe .fancybox-content {
		& { 
      width:100%;
      padding-top:56.25%; 
    }
	}
}

.hero {
  &.hero--maintain_ratio {
    .hero__inner {
      position: absolute;
      top: 0;
      left:0;
      right:0;
      bottom: 0;
    }
  }

  .hero__inner {
    display: flex;
    height: 100%;

    &.new {
      display: block;
      height: 100%;
    }
    
    @include media-query($max,$md) { 
      margin-left: auto;
      margin-right: auto;
    }

    &.height-center {
      align-items: center;
    }
    &.height-top {
      vertical-align: top;
      align-items: start;
    }
    &.height-bottom {
      vertical-align: bottom;
      align-items: flex-end;
    }

    @include media-query($max,$md) { 
      &.height-mobile-center {
        align-items: center;
      }
      &.height-mobile-top {
        align-items: start;
      }
      &.height-mobile-bottom {
        align-items: flex-end;
      }
    }
  }
}

.text-mobile-center {
  @include media-query($max,$md) { 
    text-align: center!important;
  }
}

.text-mobile-left {
  @include media-query($max,$md) { 
    text-align: left!important;
  }
}

.text-mobile-right {
  @include media-query($max,$md) { 
    text-align: right!important;
  }
}

.d-grid {
  display: grid;
}

.align-items-bottom {
  -webkit-box-align: flex-end!important;
  -ms-flex-align: flex-end!important;
  align-items: flex-end!important;
}

.secondary.logo {
  display: none;
}

.desktop-only {
  @include media-query($max,$md) {
    display: none!important;
  }
}

.mobile-only {
  @include media-query($min,$md) {
    display: none!important;
  }
}

.featured-collection {
  .grid-view-item__title {
    margin-bottom: 15px;
  }

  .grid--view-items {
    .grid__item {
      padding-bottom: $gutter-site/2;
    }
  }
}

.site-footer {
  .footer-contents {
    .footer-col {
      @include media-query($max,$md) {
        margin-bottom: 6px;
        &.open {
          .footer-accordion {
            &::after {
              content: '-';
            }
          }
        }
      }
      .footer-accordion {
        position: relative;
        @include media-query($max,$md) {
          div {
            width: 100%!important;
          }
          &::after {
            content: '+';
            float: right;
            font-family: Poppins,sans-serif;
            font: unset;
            position: absolute;   
            top:0;
            right:0;       
          }
        }
      }

      .site-footer__linklist {

        @include media-query($min,$md) {
          display: block!important;
        }
        @include media-query($max,$md) {
          display: none;
          padding-bottom: $gutter-site/2;
        }
      }
    }
  }

  a, p {
    font-family: $footer-font;
  }

  .site-footer__newsletter {
    @include media-query($max,$md) {
        max-width: unset;
    }

    label {
      margin-bottom: 0;
    }

    label, p {
      width: 100%;
    }

    input {
      background-color: #fff;
      padding: 0 15px;
    }
  }

  .logo--footer {
    margin-top: 30px;
    margin-bottom: 0;
    padding: 0 20px;
  }
}

.collection-hero__title-wrapper {
  display: flex;
  align-items: center;
  height: 100%;

  .collection-hero__title {
    font-weight: bold;
    text-transform: uppercase;
    color: #fff;
  }
}

.mobile-filter-modal-open {
  overflow-y: hidden!important;

  #Collection {
    .filter-sidebar-container {
      @include media-query($max,$md) {
        display: flex;
      }

    }
  }
}

#Collection, #search-template {
  padding: 30px 0;

  .filter-sidebar-container {
    transition: 0.5s ease;

    @include media-query($max,$md) {
      overflow-y: scroll;
      display: none;
      z-index: 1;
      top: 131px;
      padding: 0;
      background: #fff;

      position: fixed;
      z-index: 1;

      // display: flex;
      height: 100%;

      .filters-toolbar {
        overflow-y: scroll;

        .closebtn {
          padding: 10px 0;
        }
      }
    }

    .filters-toolbar {
      &.is_sticky {
        position: sticky;
      }
    }    
  }

  &.hide_side_filter {
    .filter-sidebar-container {
      max-width: 0;
      visibility: hidden;
      opacity: 0;
      padding: 0;
    }
  }

  .grid-view-item {
    .grid-view-item__title {
      padding: 10px;
      position: relative;
    }
  }

  .filters-toolbar {
    color: #000;
    padding: 0 20px;

    label[for] {
      margin-left: 5px;
      text-transform: uppercase;
    }

    .full.sidebar-filter {
      margin-bottom: 20px;
      padding-bottom: 10px;
      border-bottom: 3px solid;

      &:not(.radiobutton) {
        input[type="checkbox"] {
          -webkit-appearance: unset;
          -moz-appearance: unset;
          -ms-appearance: unset;     /* not currently supported */
          -o-appearance: unset;      /* not currently supported */
          display: none;
        }
      }

      &.checkbox {
        input[type="checkbox"] {
          width: 20px;
          height: 20px;
          vertical-align: top;
          border: solid 1px #454142;
          border-radius: 4px;
          margin-right: 6px;
          display: inline-block;
          position: relative;    
          &:checked {
            background-color: #111111;
            &::before {
              content: '✓';
              width: 6px;
              height: 6px;
              top: -2px;
              left: 4px;
              font-size: 15px;
              color: white;
              position: absolute;
            }
          }
        }
      }
      
      &.color_swatch {
        input[type="checkbox"] {
          & + label {
            width: 35px;
            height: 35px;
            text-indent: -1000px;
            border-radius: 50px;
            margin-left: 0;
            border: 3px solid #fff;
          }
          &:checked + label {
            border-color: #000;
          }
        }
      }

      &.button, &.color_swatch {
        .option_buttons {
          display: flex;
          flex-wrap: wrap;
    
          .filter-element {
            padding: 2px 4px 2px 0;
          }
        }
      }

      &.button input[type="checkbox"]  + label , &.current_filter .current_filter_btn { 
        min-width: 94px;
        border: 1px solid #454142;
        text-align: center;
        padding: 7px 5px;
        font-size: 12px;
        margin-left: 0;        
      }

      &.button input[type="checkbox"]:checked + label {
        color: #fff;
        background-color: #454142;
      }

      &.current_filter {
        &:not(.show) {
          .option_buttons {
            display: none;
          }
        }        

        .current_filter_btn {
          margin: 2px 4px 2px 0;
          .fal {
            font-weight: bold;
            margin-left: 3px;
          }
          &.clear_btn {
            background-color: #000;
            border-color: #000;
            color: #fff;
          }
        }
      }

      .filter-element {
        display: none;
      }

      &.show {
        .filter-element {
          display: block;
          &.all {
            display: none;
          }
        }
        h5 {
          &:after {
            content: '-';
          }
        }
      }

      h5 {
        text-transform: uppercase;
        font-weight: bold;
        position: relative;
        cursor: pointer;

        &:after {
          content: '+';
          font-size: 24px;
          right: 0;
          position: absolute;
        }
      }
    }
  }
}

